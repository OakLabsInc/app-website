{
  "name": "app-website",
  "version": "1.0.0",
  "description": "App for just a simple website in OakOS",
  "main": "src/index.js",
  "engines": {
    "node": "10.2.1"
  },
  "dependencies": {
    "standard": "~12.0.1"
  },
  "devDependencies": {
    "oak": "~5.0.0-rc0"
  },
  "scripts": {
    "start": "node_modules/.bin/oak src/index.js",
    "rebuild": "/bin/bash -c 'node_modules/.bin/oak-rebuild'",
    "dev": "export $(cat .env | xargs) && npm start",
    "test-standard": "node_modules/.bin/standard",
    "clean-install": "rm -rf node_modules package-lock.json && npm i && npm run rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oaklabsinc/app-website.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/oaklabsinc/app-website/issues"
  },
  "homepage": "https://github.com/oaklabsinc/app-website#readme",
  "private": true
}
