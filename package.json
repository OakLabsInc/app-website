{
  "name": "app-website",
  "version": "1.0.2",
  "description": "App for just a simple website in OakOS",
  "main": "src/index.js",
  "engines": {
    "node": "10.2.1"
  },
  "dependencies": {
    "standard": "~12.0.1"
  },
  "devDependencies": {
    "oak": "~5.0.9"
  },
  "scripts": {
    "start": "node_modules/.bin/oak .",
    "dev": "env $(cat .env | grep -v '#' | xargs) npm start",
    "rebuild": "rm -rf node_modules/ package-lock.json && npm i && node_modules/.bin/oak-rebuild $(pwd)",
    "test": "exit 0;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oaklabsinc/app-website.git"
  },
